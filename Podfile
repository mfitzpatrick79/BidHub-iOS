# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
inhibit_all_warnings!
use_frameworks!

target 'AuctionApp' do
  pod 'Parse'
  pod 'AFNetworking', :git => 'https://github.com/AFNetworking/AFNetworking.git'
  pod 'SVProgressHUD', :git => 'https://github.com/SVProgressHUD/SVProgressHUD.git'
  pod 'AFViewShaker', '>= 0.0.2'
  pod 'IHKeyboardAvoiding', '>= 1.1'
  pod 'NSDate+RelativeTime', '>= 1.0'
  pod 'UIImage-Resize', '>= 1.0'
  pod 'CSNotificationView', '>= 0.5'
  pod 'HanekeSwift', :git => 'https://github.com/Haneke/HanekeSwift.git'
  pod 'OneSignal', '>= 2.5.2', '< 3.0'
  pod 'PhoneNumberKit', '>= 1.1.1'
end

target 'OneSignalNotificationServiceExtension' do
    pod 'OneSignal', '>= 2.5.2', '< 3.0'
end
